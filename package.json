{
  "name": "mate-finder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "all": "concurrently \"npm run dev:client\" \"npm run dev:ApiServer\" \"npm run dev:SteamAuth\" \"npm run dev:imageStorage\" \"npm run dev:webSocket\"",
    "dev:client": "cd ..\\mate-finder-client && npm run --host dev",
    "dev:ApiServer": "cd ..\\mate-finder-server && npm run dev",
    "dev:SteamAuth": "cd ..\\steam-auth-server && npm run dev",
    "dev:imageStorage": "cd ..\\image-storage && npm run dev",
    "dev:webSocket": "cd ..\\mate-finder-websocket && npm run dev",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier --write ./src"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.6",
    "@mui/material": "^5.15.1",
    "@mui/x-date-pickers": "^6.18.5",
    "@react-spring/web": "^9.7.3",
    "@reduxjs/toolkit": "^2.0.1",
    "@types/js-cookie": "^3.0.6",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "firebase": "^10.8.0",
    "js-cookie": "^3.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.0",
    "react-select": "^5.8.0",
    "socket.io-client": "4.7.1",
    "styled-components": "^6.1.1",
    "sweetalert2": "^11.6.13"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "3.2.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
